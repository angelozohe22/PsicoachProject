<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.psicoachproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature android:name="android.hardware.camera2"
        android:required="true"/>

    <application
        android:name=".core.aplication.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PsicoachProject">
        <activity android:name=".ui.modules.home.psicosec.activities.DetailEventActivity"></activity>
        <activity android:name=".ui.modules.home.client.activities.PagosActivity" />
        <activity android:name=".ui.modules.home.client.activities.DatosActivity" />
        <activity android:name=".ui.modules.home.client.activities.PromotionActivity" />
        <activity
            android:name=".ui.modules.main.activities.SplashActivity"
            android:theme="@style/Theme.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.modules.home.HomeActivity" />
        <activity android:name=".ui.modules.main.activities.SignUpActivity" />
        <activity android:name=".ui.modules.main.activities.SignInActivity" />
        <activity android:name=".ui.modules.main.activities.MainActivity" />
    </application>

</manifest>